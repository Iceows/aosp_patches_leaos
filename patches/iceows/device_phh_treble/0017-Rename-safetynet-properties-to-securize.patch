From d149c772bdd028cf68db974fd543094fe936ca15 Mon Sep 17 00:00:00 2001
From: Raphael Mounier <mounierr07@gmail.com>
Date: Sat, 15 Apr 2023 12:46:08 +0200
Subject: [PATCH] Rename safetynet properties to securize

---
 phh-prop-handler.sh | 4 ++--
 vndk.rc             | 4 ++--
 2 files changed, 4 insertions(+), 4 deletions(-)

diff --git a/phh-prop-handler.sh b/phh-prop-handler.sh
index 8c46052..ac38415 100644
--- a/phh-prop-handler.sh
+++ b/phh-prop-handler.sh
@@ -205,8 +205,8 @@ if [ "$1" == "persist.sys.phh.qin.dt2w" ];then
     exit
 fi
 
-#safetynet
-if [ "$1" == "persist.sys.phh.safetynet" ]; then
+#safetynet/securize
+if [ "$1" == "persist.sys.phh.securize" ]; then
     if [[ "$prop_value" != "true" ]]; then
         exit 1
     fi
diff --git a/vndk.rc b/vndk.rc
index 2efbc4b..aa6e247 100644
--- a/vndk.rc
+++ b/vndk.rc
@@ -64,8 +64,8 @@ on property:persist.sys.phh.disable_a2dp_offload=1
 on property:persist.sys.phh.disable_a2dp_offload=*
     setprop persist.bluetooth.bluetooth_audio_hal.disabled ${persist.sys.phh.disable_a2dp_offload}
 
-on property:persist.sys.phh.safetynet=*
-    exec u:r:phhsu_daemon:s0 root -- /system/bin/phh-prop-handler.sh "persist.sys.phh.safetynet"
+on property:persist.sys.phh.securize=*
+    exec u:r:phhsu_daemon:s0 root -- /system/bin/phh-prop-handler.sh "persist.sys.phh.securize"
 
 on property:persist.sys.phh.autorun=*
     exec u:r:phhsu_daemon:s0 root -- /system/bin/phh-prop-handler.sh "persist.sys.phh.autorun"
-- 
2.25.1

